@model RegisterInternDto

<div class="container" style="margin: 16px 0">
	<form method="post" enctype="multipart/form-data">
		<div class="text text-danger" asp-validation-summary="ModelOnly"></div>
		<div class="form-group">
			<label asp-for="FullName">FullName:</label>
			<input class="form-control" asp-for="FullName" />
			<span asp-validation-for="FullName" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Email">Email:</label>
			<input type="email" class="form-control" asp-for="Email" />
			<span asp-validation-for="Email" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="PhoneNumber">PhoneNumber:</label>
			<input class="form-control" asp-for="PhoneNumber" />
			<span asp-validation-for="PhoneNumber" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Password">Password:</label>
			<input type="password" class="form-control" asp-for="Password" />
			<span asp-validation-for="Password" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="BirthYear">BirthYear:</label>
			<input class="form-control" asp-for="BirthYear" />
			<span asp-validation-for="BirthYear" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Experience">Experience:</label>
			<input class="form-control" asp-for="Experience" />
			<span asp-validation-for="Experience" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="BirthYear">BirthYear:</label>
			<input class="form-control" asp-for="BirthYear" />
			<span asp-validation-for="BirthYear" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="FieldsOfLaw">FieldsOfLaw:</label>
			<input class="form-control" asp-for="FieldsOfLaw" />
			<span asp-validation-for="FieldsOfLaw" class="text text-danger"></span>
		</div>
		<div class="form-group" asp-validation-summary="ModelOnly"></div>
		@Html.AntiForgeryToken()
		<button type="submit" style="width: 180px; margin-top: 16px;" class="btn btn-success btn-rounded">Create User</button>
	</form>
</div>