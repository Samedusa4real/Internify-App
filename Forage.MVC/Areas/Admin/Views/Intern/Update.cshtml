@model InternUpdateDto

<div class="container" style="margin: 16px 0;">
	<form method="post" enctype="multipart/form-data">
		<div class="text text-danger" asp-validation-summary="ModelOnly"></div>
		<div class="form-group">
			<label asp-for="Name">Name:</label>
			<input class="form-control" asp-for="Name" />
			<span asp-validation-for="Name" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Surname">Surname:</label>
			<input class="form-control" asp-for="Surname" />
			<span asp-validation-for="Surname" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Gender">Gender:</label>
			<input class="form-control" asp-for="Gender" />
			<span asp-validation-for="Gender" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="PhoneNumber">PhoneNumber:</label>
			<input class="form-control" asp-for="PhoneNumber" />
			<span asp-validation-for="PhoneNumber" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Email">Email:</label>
			<input class="form-control" asp-for="Email" />
			<span asp-validation-for="PhoneNumber" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="UniversityName">UniversityName:</label>
			<input class="form-control" asp-for="UniversityName" />
			<span asp-validation-for="UniversityName" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="UniStartYear">UniStartYear:</label>
			<input class="form-control" asp-for="UniStartYear" />
			<span asp-validation-for="UniStartYear" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="UniGraduationYear">UniGraduationYear:</label>
			<input class="form-control" asp-for="UniGraduationYear" />
			<span asp-validation-for="UniGraduationYear" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="@Model.file">Image:</label>
			<input type="file" class="form-control" asp-for="@Model.file" />
			<span asp-validation-for="file" class="text text-danger"></span>
		</div>
		@Html.AntiForgeryToken()
		<button type="submit" style="width: 180px; margin-top: 16px;" class="btn btn-success btn-rounded">Update</button>
	</form>
</div>